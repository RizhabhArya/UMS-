# Server configuration
PORT=8080

# MongoDB Atlas connection string
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.mongodb.net/UMS_DB?retryWrites=true&w=majority

# Frontend origin for CORS (update this in production)
CORS_ORIGIN=http://localhost:5173

# JWT Auth secrets
ACCESS_TOKEN_SECRET=super-secret-access-key
ACCESS_TOKEN_EXPIRY=15m

REFRESH_TOKEN_SECRET=super-secret-refresh-key
REFRESH_TOKEN_EXPIRY=7d

# Cloudinary config
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=123456789012345
CLOUDINARY_API_SECRET=very_secret_cloud_key
